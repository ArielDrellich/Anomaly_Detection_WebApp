<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Test Server | <%= title %></title>
    </head>
    <body>
        <%- include('./partials/nav.ejs') %>

        <div class="create-model content">
            <form>
                <label for="title">Model title:</label>
